<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>syslights</string>
    <key>Program</key>
    <string>/path/to/syslights_server.py</string>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/syslights.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/syslights_err.log</string>
</dict>
</plist>
